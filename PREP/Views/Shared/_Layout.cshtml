<!DOCTYPE html>
<html lang="en">
    <head>
   <meta charset="utf-8" />
    <title>@ViewBag.PREP</title>
    <meta http-equiv="X-UA-Compatible" content="IE=9" />
    <link href="~/favicon.ico" rel="Shortcut Icon" type="image/x-icon" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet"/>

    @Styles.Render("~/Content/bootstrap")
    @Styles.Render("~/Content/fullcalendarcss")
    @Styles.Render("~/Content/customcss")

</head>
<body>
    <div class="container">
        <div class="masthead">
            @*<h3 class="text-muted">SAPD Activity Application</h3>*@
            <nav class="navbar navbar-default">
        <div class="container-fluid">
          <div class="navbar-header">
            <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
              <span class="sr-only">Toggle navigation</span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
              <span class="icon-bar"></span>
            </button>
            <a class="navbar-brand" href="#">Grant Web</a>
          </div>
          <div id="navbar" class="navbar-collapse collapse">
            <ul class="nav navbar-nav">
              <li class="active"><a href="~/Home/Index"><span class="fa fa-home fa-fw"></span>Home</a></li>
              <li><a href="~/Home/About">About</a></li>
              <li><a href="~/Home/Contact"><i class="fa fa-phone"></i>&nbsp;Contact</a></li>
             
            </ul>
           
          </div><!--/.nav-collapse -->
        </div><!--/.container-fluid -->
      </nav>
        </div>
        <!-- Jumbotron -->
        <div class="jumbotron">
            <h1>
                PREP
            </h1>
            <p class="lead">
                Scheduling & organizing Public Resource Educational Packages
            </p>
            <p>
                <a class="btn btn-lg btn-success" href="#" role="button">Get started</a></p>
        </div>
        <div class="container">
            <div class="row">
                @*<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">*@
                @RenderBody()
                <hr />
                @* </div>*@
            </div>
        </div>
        <!-- Site footer -->
        <footer class="footer">
        <p class="text-center">&copy; @DateTime.Now.Year - Activity App - Created by: D.Davidson#1341</p>
      </footer>
    </div>
    <!-- /container -->
    @Scripts.Render("~/bundles/fullcalendarjs")
    @Scripts.Render("~/bundles/modernizr")
    @Scripts.Render("~/bundles/moment")
    
    @Scripts.Render("~/bundles/googlejquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    
   @* @Scripts.Render("~/bundles/datepick")*@
</body>
</html>
